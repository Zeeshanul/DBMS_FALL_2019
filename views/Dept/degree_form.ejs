    <input type="text" name="progid" id="progid" placeholder="Progra ID i.e BCE">

    <hr>
    <label for="progid">Year</label><br>
    <input type="number" name="Year" id="Year" placeholder="2016">

    <hr>
    <label for="cid">Course ID</label><br>
    <input type="text" name="cid" id="cid" placeholder="CS222">

    <hr>
    <label for="type">Course type</label><br>
    <input type="text" name="type" id="type" placeholder="Core/Elective">
    <hr>
    <label for="cid">Offered by</label><br>
    <input type="text" name="Offered" id="Offered" placeholder="FCSE">
    <hr>
    <label for="cid">semester# by</label><br>
    <input type="number" name="semnum" id="semnum" placeholder="8">
    <hr>
    <label for="cid">Semester Type </label><br>
