<table>
    <tr>
        <th>PROG_ID</th>
        <th>YEAR</th>
        <th>COURSE_ID</th>
        <th>COURSE_TYPE</th>
        <th>OFFERED_BY</th>
        <th>SEMESTER#</th>
        <th>SEMESTER_TYPE</th>

    </tr>
    <hr>
    <%for (i=0;i<result.length;i++){%>

    <tr>
        <% for (j=0;j<result[0].length;j++){%>
        <td><%=result[i][j]%></td>
        <%}%>
        <td>
            <form action="/degree-edit/<%=result[i][0]%>/<%=result[i][1]%>/<%=result[i][2]%>">
        <button type="submit">Edit</button>
        </form>
        </td>
        <td>
            <form action="/degree-delete/<%=result[i][0]%>/<%=result[i][1]%>/<%=result[i][2]%>"><button
                    type="submit">Delete</button></form>
        </td>

    </tr>

    <%}%>

        <br>
        <hr>


</table>

<form action="/degree/<%=result%>" method="POST">
